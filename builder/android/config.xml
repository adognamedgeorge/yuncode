<?xml version='1.0' encoding='utf-8'?>
<widget id="com.yunmayi.android" version="10.3.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>云蚂蚁</name>
    <description>
        云蚂蚁App
    </description>
    <author email="381266902@qq.com" href="http://www.yunmayi.com">
        Yunmayi Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <chcp>
        <native-interface version="1" />
        <config-file url="http://api.h5.yunmayi.com/app/yunmayi-app/chcp.json" />
        <auto-download enabled="true" />
        <auto-install enabled="true" />
    </chcp>
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="ShowSplashScreen" value="true" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="FadeSplashScreen" value="false" />
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
    <plugin name="cordova-plugin-wechat" spec="^2.0.0">
        <variable name="WECHATAPPID" value="wx6f78c0964351ea05" />
    </plugin>
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <plugin name="cordova-plugin-screen-orientation" spec="^3.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-alipay-v2" spec="0.0.4">
        <variable name="APP_ID" value="2015121600986717" />
    </plugin>
    <plugin name="cordova-hot-code-push-plugin" spec="^1.5.3" />
    <plugin name="cordova-plugin-qrscanner" spec="https://github.com/chenwenzhang/cordova-plugin-qrscanner.git" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.1" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.1.0" />
    <plugin name="cordova-plugin-app-update" spec="^1.6.0" />
    <engine name="android" spec="^6.4.0" />
</widget>
